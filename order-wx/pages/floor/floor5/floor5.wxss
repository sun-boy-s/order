/* pages/floor/floor1/floor1.wxss */
.page-body{
  width: 100%;
  height: 100%;
}
.map {
  width: 100%;
  height: auto;
}
.moveArea{
  width: 100%;
  height: 500rpx;
  border: 1rpx red solid;
}
.moveView{
  width: 100rpx;
  height: 100rpx;
  border: 0.5rpx red solid;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.img{
  width: 5rpx;
  height: 5rpx;
  flex-grow: 1;
  /* flex-shrink: 1; */
}
.img .img-if{
  width: 5rpx;
  height: 5rpx;
}
.img .img-if image{
  width: 100%;
  height: 100%;
  display: block;
}
.moveView .left-part{
  width: 30%;
  height: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.moveView .left-part .left {
  width: 33%;
  height: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.moveView .left-part .left .arrayOne{
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
}
.moveView .left-part .left .arrayTwo{
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
}

