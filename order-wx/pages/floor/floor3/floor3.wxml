                  </view>
                </view>
              </view>
            </view>
            <view class="middle-blank"></view>
            <view class="bottom">
              <view class="left">
                <view class="arrayThirt">
                  <view wx:for="{{seatIdList.arrayThirt}}" wx:key='key' wx:for-item='item' class="img">
