                    <view wx:if="{{item.ordering==0}}" class="img-if">
                      <image src="../../../utils/images/seat-right.png" bindtap="imgClick" id="{{item.seat_id}}"></image>
                    </view>
                    <view wx:else class="img-if">
                      <image src="../../../utils/images/seat-right-select.png" id="{{item.seat_id}}"></image>
                    </view>
                  </view>
                </view>
              </view>
            </view>
          </view>
          <view class="right-part">

            <view class="left">
              <view class="arrayOne">
                <view wx:for="{{seatIdList.arrayFive}}" wx:key='key' wx:for-item='item' class="img">
                  <view wx:if="{{item.ordering==0}}" class="img-if">
                    <image src="../../../utils/images/seat-left.png" bindtap="imgClick" id="{{item.seat_id}}"></image>
                  </view>
